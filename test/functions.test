function add(x, y) {
    return x + y;
}

function multiply(x, y) {
    return x * y;
}

function subtract(x, y) {
    return x - y;
}

function divide(x, y) {
    return x / y;
}

function div(x, y) {
    if (y > 0) {
        return x / y;
    } else {
        return 1;
    }
}

